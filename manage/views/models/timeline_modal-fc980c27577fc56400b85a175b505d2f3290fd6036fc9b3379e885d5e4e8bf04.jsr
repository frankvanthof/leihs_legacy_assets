<div class='modal large hide fade ui-modal padding-inset-m padding-horizontal-l' role='dialog' tabindex='-1'>
  <div class='row padding-vertical-m'>
    <div class='col1of2'>
      <h3 class='headline-l'>{{jed "Availability Timeline"/}}</h3>
      <h3 class='headline-s light'>{{>name()}}</h3>
    </div>
    <div class='col1of2'>
      <div class='float-right'>
        <a aria-hidden='true' class='modal-close weak' data-dismiss='modal' title='{{jed "close dialog"/}}' type='button'>
          {{jed "Cancel"/}}
        </a>
        <button aria-hidden='true' class='button modal-close text-ellipsis weak white' data-dismiss='modal' title='{{jed "close dialog"/}}' type='button'>
          {{jed "Close"/}}
        </button>
      </div>
    </div>
  </div>
  <div class='row margin-top-m'>
    <div class='modal-body no-padding'>
      <div class='row'>
        <div class='position-absolute-topright width-full text-align-center'>
          <div id='loading'>
            <div class='height-m'></div>
            {{partial "views/loading"/}}
          </div>
        </div>
        <iframe height='500px' id='timeline' src='/manage/{{current_inventory_pool_id/}}/models/{{>id}}/timeline' width='100%'></iframe>
      </div>
      <div class='row text-align-right'>
        <a class='button white modal-close' href='/manage/{{current_inventory_pool_id/}}/models/{{>id}}/timeline' target='_blank'>
          <i class='fa fa-resize-full'></i>
        </a>
      </div>
    </div>
  </div>
</div>
