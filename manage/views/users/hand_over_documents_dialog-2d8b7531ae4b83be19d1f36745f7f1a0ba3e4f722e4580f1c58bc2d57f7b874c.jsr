<div class='modal fade ui-modal medium min-height-m' role='dialog' tabindex='-1'>
  <div class='modal-header row'>
    <div class='col3of5'>
      <h2 class='headline-l'>{{jed "Hand over completed"/}}</h2>
      <h3 class='headline-m light'>
        {{jed itemsCount "%s item" "%s items" itemsCount/}}
        {{jed "to %s" contract.user().name()/}}
      </h3>
    </div>
    <div class='col2of5 text-align-right'>
      <div class='multibutton'>
        <a class='button white text-ellipsis' href='{{dailyPath/}}'>{{jed "Finish this hand over"/}}</a>
        <div class='dropdown-holder inline-block'>
          <div class='button white dropdown-toggle'>
            <div class='arrow down'></div>
          </div>
          <ul class='dropdown right'>
            <li>
              <a class='dropdown-item' href=''>
                <i class='fa fa-undo'></i>
                {{jed "Back to this user"/}}
              </a>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <div class='modal-body'>
    <div class='padding-inset-l margin-top-s'>
      <div class='row emboss padding-inset-s margin-bottom-s'>
        <div class='col1of3'></div>
        <div class='col1of3'>
          <a class='button white width-full' href='{{>contract.url()}}' target='_blank'>
            <i class='fa fa-file-alt'></i>
            {{jed "Contract"/}}
          </a>
        </div>
        <div class='col1of3'></div>
      </div>
      <div class='row emboss padding-inset-s margin-bottom-s'>
        <div class='col1of3'></div>
        <div class='col1of3'>
          <a class='button white width-full' href='{{>contract.url('value_list')}}' target='_blank'>
            <i class='fa fa-list-ol'></i>
            {{jed "Value List"/}}
          </a>
        </div>
        <div class='col1of3'></div>
      </div>
    </div>
  </div>
</div>
