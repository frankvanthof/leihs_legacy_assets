<li class='separated-bottom exclude-last-child'>
  <a class='row' title='{{>firstname}} {{>lastname}}'>
    <div class='row text-ellipsis'>
      <strong>
        {{>firstname}}
        {{>lastname}}
      </strong>
    </div>
    <div class='row text-ellipsis'>
      {{>address}}{{>city}}
    </div>
  </a>
</li>
