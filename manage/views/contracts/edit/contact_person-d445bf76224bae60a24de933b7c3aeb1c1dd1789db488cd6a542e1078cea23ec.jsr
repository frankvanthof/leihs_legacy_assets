<div class='row padding-vertical-m' id='contact-person'>
  <div class='row emboss padding-inset-m'>
    <div class='col4of9 text-align-center' id='swapped-person'>
      <input autocomplete='off' class='width-m' id='user-id' placeholder='{{jed 'Contact person'/}} {{jed 'Name / ID'/}}' type='text'>
      <div id='selected-user'></div>
    </div>
    <div class='col1of9 text-align-center'>
      <i class='fa fa-exchange icon-xxl'></i>
    </div>
    <div class='col4of9 text-align-center'>
      {{if delegatedUser()}}
      <p class='paragraph-s padding-top-xxs margin-top-xxs'>
        <strong>
          {{>delegatedUser().firstname}}
          {{>delegatedUser().lastname}}
        </strong>
      </p>
      {{/if}}
    </div>
  </div>
</div>
